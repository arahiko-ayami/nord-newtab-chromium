{
  "name": "Nord New Tab",
  "description": "Replace Chrome Newtab with better Nord theme",
  "version": "0.1.0",
  "manifest_version": 3,
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions" : [
    "search"
  ],
  "icons": {
    "16": "nord.png",
    "32": "nord.png",
    "48": "nord.png",
    "128": "nord.png"
  }
}